function put_char(c,x,y,fg,bg)
	fg = fg or def_fg
	bg = bg or def_bg
	x = x*tile_w
	y = y*tile_h
	rectfill(x,y,x+tile_w,y+tile_h,bg)
	print(c,x+1,y+1,fg)
end

function set_default_bg(bg)
	def_bg = bg
end

function set_default_fg(fg)
	def_fg = fg
end
